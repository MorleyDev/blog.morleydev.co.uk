{
  "name": "blog.morleydev.co.uk",
  "version": "1.0.0",
  "description": "",
  "main": "",
  "scripts": {
    "postinstall": "jspm install -q && concurrently \"npm run typings\" \"npm run compile-html\"",
    "prepublish": "jspm build scripts/index.ts --skip-source-maps --minify --skip-rollup app/index.min.js",
    "typings": "node typings.js && cd app && typings install && cd ..",
    "compile-html": "node utilities/compile.js",
    "serve": "firebase serve",
    "develop": "npm run postinstall && concurrently \"npm run serve\" \"npm run watch\"",
    "deploy": "npm install && firebase deploy",
    "test": "jspm run test/unit.ts | tap-dot",
    "watch:code": "jspm build scripts/index.ts -wd --skip-rollup app/index.min.js",
    "watch:test": "watch \"npm run test\" ./app/scripts/",
    "watch": "concurrently \"npm run watch:code\" \"npm run watch:test\""
  },
  "typingsDependencies": {
    "repo": {
      "dependencies": {
        "immutable": "registry:npm/immutable#3.7.6+20160411060006",
        "redux": "registry:npm/redux#3.0.6+20160214194820",
        "firebase": "registry:npm/firebase#3.0.5+20160705215914",
        "tape": "registry:npm/tape#4.0.0+20160723033700"
      },
      "globalDependencies": {
        "crossroads": "registry:dt/crossroads#0.0.0+20160317120654",
        "js-signals": "registry:dt/js-signals#0.0.0+20160316155526",
        "node": "registry:dt/node#6.0.0+20160613154055",
        "react": "registry:dt/react#0.14.0+20160602151522",
        "react-dom": "registry:dt/react-dom#0.14.0+20160412154040",
        "systemjs": "registry:dt/systemjs#0.19.29+20160702105636",
        "redux-devtools": "registry:dt/redux-devtools#3.0.0+20160703120921",
        "redux-devtools-dock-monitor": "registry:dt/redux-devtools-dock-monitor#1.0.1+20151216103641",
        "redux-devtools-log-monitor": "registry:dt/redux-devtools-log-monitor#1.0.1+20151216103641",
        "showdown": "registry:dt/showdown#1.4.1+20160608102004"
      }
    },
    "files": {
      "dependencies": {
        "typemoq": "jspm_packages/npm/typemoq@VERSION/typemoq.node.d.ts",
