    "overrides": {
      "github:HubSpot/tether@1.3.3": {
        "main": "js/tether",
        "directories": {
          "lib": "dist"
        },
        "meta": {
          "js/tether.js": {
            "exports": "Tether",
            "format": "global"
          }
        }
      },
      "github:firebase/firebase-bower@3.2.1": {
        "main": "firebase.js",
        "registry": "jspm",
        "meta": {
          "firebase.js": {
            "exports": "firebase",
            "format": "global"
          }
        }
      },
      "npm:bootstrap@4.0.0-alpha.3": {
        "main": "dist/js/bootstrap",
        "jspmNodeConversion": false,
        "directories": {
          "lib": "."
        },
        "dependencies": {
          "jquery": "*",
          "tether": "github:HubSpot/tether@^1.1.1"
        },
        "meta": {
          "dist/js/bootstrap.js": {
            "deps": [
              "jquery",
              "tether"
            ],
            "exports": "$",
            "format": "global"
          }
        }
      },
      "npm:browserify-zlib@0.1.4": {
        "dependencies": {
          "readable-stream": "^2.0.2",
          "pako": "~0.2.0"
        },
        "map": {
          "_stream_transform": "readable-stream/transform"
        }
      },
      "npm:debug@2.2.0": {
        "main": "browser.js",
        "jspmNodeConversion": false,
        "format": "cjs",
        "map": {
          "./browser.js": {
            "node": "./node.js"
          },
          "fs": "@node/fs",
          "net": "@node/net",
          "tty": "@node/tty",
          "util": "@node/util"
        }
      },
      "npm:font-awesome@4.6.3": {
        "main": "css/font-awesome.css!",
        "jspmNodeConversion": false,
        "registry": "jspm",
        "dependencies": {
          "css": "0.1"
        }
      },
      "npm:inherits@2.0.1": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:jquery@3.1.0": {
        "format": "amd"
      },
      "npm:lodash@4.14.1": {
        "map": {
          "buffer": "@empty",
          "process": "@empty"
        }
      },
      "npm:ms@0.7.1": {
        "jspmNodeConversion": false,
        "format": "cjs"
      },
      "npm:sass.js@0.9.11": {
        "map": {
          "ws": "@empty"
        },
        "meta": {
          "dist/sass.worker.js": {
            "format": "global"
          }
        }
      },
      "npm:typescript@2.0.0": {
        "browser": {},
        "map": {
          "buffer": "@empty",
          "child_process": "@empty",
          "fs": "@empty",
          "path": "@empty",
          "process": "@empty",
          "readline": "@empty"
        }
      }
    }
